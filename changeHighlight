Sub changeHighlight()
    'ws as a worksheet object
    Dim ws As Worksheet
    
    'cell as a range object to iterate through each cell
    Dim cell As Range
    
    'storing original row color and replacing with new
    Dim rowColor As Long
    Dim newColor As Long
    
    'modify hydrants worksheet
    Set ws = ThisWorkbook.Sheets("testing")
    
    'get original red color
    rowColor = RGB(153, 255, 204)
    
    'define a better color
    newColor = RGB(153, 255, 204)
    
    'loop through each cell in range
    For Each cell In ws.UsedRange
        'change row color
        If cell.Interior.Color = rowColor Then
            cell.EntireRow.Interior.Color = newColor
        End If
    Next cell
    
    MsgBox "Row color has been changed!"
End Sub

